<%-include('components/header', {title: 'MainPage'})%>
    <main>
       <div class="messages">
            <% messages.forEach(message => { %>
                <div class="message">
                    <div class="header">
                        <span class="user">
                            <%= message.user %>
                        </span>
                        -
                        <span class="date">
                            <%= message.added %>
                        </span>
                    </div>
                    <div class="content" class="content">
                        <%= message.text %>
                    </div>
                </div>
            <% }) %>
       </div>
    </main>
<%-include('components/footer')%>